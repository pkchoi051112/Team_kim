│
├─ scripts/
    home.js
fetch("./data/restaurants.json")
  .then(res => res.json())
  .then(data => {
    if (navigator.geolocation) {
      navigator.geolocation.getCurrentPosition(pos => {
        const userLat = pos.coords.latitude;
        const userLng = pos.coords.longitude;

        const map = new kakao.maps.Map(document.getElementById("map"), {
          center: new kakao.maps.LatLng(userLat, userLng),
          level: 3
        });

        new kakao.maps.Marker({
          map: map,
          position: new kakao.maps.LatLng(userLat, userLng)
        });

        const storeList = document.getElementById("storeList");
        storeList.innerHTML = "";

        data.forEach(store => {
          const star = store.priceMin <= 7000 ? " ⭐" : "";
          const item = document.createElement("div");
          item.className = "store-item";
          item.innerHTML = `
            <div>${store.name}${star}</div>
            <div>${store.category}</div>
            <div>${store.priceMin}~${store.priceMax}원</div>
          `;
          storeList.appendChild(item);

          new kakao.maps.Marker({
            map,
            position: new kakao.maps.LatLng(store.coords.lat, store.coords.lng)
          });
        });
      });
    }
  });

    map.js
fetch("./data/restaurants.json")
  .then(res => res.json())
  .then(data => {
    const map = new kakao.maps.Map(document.getElementById("map"), {
      center: new kakao.maps.LatLng(37.3005, 126.8385),
      level: 3
    });

    const list = document.getElementById("storeList");

    data.forEach(store => {
      const star = store.priceMin <= 7000 ? " ⭐" : "";
      const item = document.createElement("div");
      item.className = "store-item";
      item.innerHTML = `
        <div>${store.name}${star}</div>
        <div>${store.category}</div>
        <div>${store.priceMin}~${store.priceMax}원</div>
      `;
      list.appendChild(item);

      new kakao.maps.Marker({
        map,
        position: new kakao.maps.LatLng(store.coords.lat, store.coords.lng)
      });
    });
  });

    value.js
fetch("./data/restaurants.json")
  .then(res => res.json())
  .then(data => {
    const list = document.getElementById("storeList");
    const valueStores = data.filter(s => s.priceMin <= 7000);

    valueStores.forEach(store => {
      const item = document.createElement("div");
      item.className = "store-item";
      item.innerHTML = `
        <div>${store.name} ⭐</div>
        <div>${store.category}</div>
        <div>${store.priceMin}~${store.priceMax}원</div>
      `;
      list.appendChild(item);
    });
  });

